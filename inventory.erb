[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v1.5.0
openshift_image_tag=v1.5.0
openshift_public_hostname=console.devopshumans.com
openshift_master_default_subdomain=cloudapps.devopshumans.com
openshift_metrics_hawkular_hostname=hawkular-metric.devopshumans.com
openshift_hosted_metrics_deploy=true
openshift_disable_check=docker_storage,memory_availability
openshift_master_overwrite_named_certificates=true
openshift_hosted_router_selector='region=infra'
openshift_hosted_metrics_deployer_version=v1.5.0

[masters]
oshift-master openshift_schedulable=true

[etcd]
oshift-master

[nodes]
oshift-master openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
oshift-compute openshift_node_labels="{'region': 'compute', 'zone': 'default'}" openshift_schedulable=true

