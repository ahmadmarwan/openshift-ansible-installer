[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v1.5.0
openshift_image_tag=v1.5.0
openshift_public_hostname=console.devopshumans.com
openshift_master_default_subdomain=cloudapps.devopshumans.com
openshift_hosted_metrics_deploy=true
openshift_disable_check=docker_storage,memory_availability
openshift_master_overwrite_named_certificates=true
openshift_hosted_metrics_storage_kind=nfs
openshift_hosted_metrics_storage_access_modes=['ReadWriteOnce']
openshift_hosted_metrics_storage_host=console.devopshumans.com
openshift_hosted_metrics_storage_nfs_directory=/nfs/metrics
openshift_hosted_metrics_storage_volume_name=metrics
openshift_hosted_metrics_storage_volume_size=10Gi
openshift_hosted_metrics_storage_labels={'storage': 'metrics'}


[masters]
console.devopshumans.com openshift_schedulable=true

[etcd]
console.devopshumans.com

[nodes]
openshift-compute-east1-b.devopshumans.com openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
openshift-compute-east1-c.devopshumans.com openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true

